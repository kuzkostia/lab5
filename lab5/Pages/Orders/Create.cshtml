@page
@model lab5.Pages.Users.CreateModel

<h1>Add new order</h1>
<form method="post">
  <div class="form-group row">
	  <label>Number</label>
	  <div class="col-sm-10">
		  <select asp-for="Number">
			<option>Choose number</option>
		  </select>
		  <span asp-validation-for="Number"></span>
	  </div>
  </div>
  <div class="form-group row">
		<label>Price</label>
		<div class="col-sm-10">
			<select asp-for="Price">
				<option>Choose price</option>
			</select>
			<span asp-validation-for="Price"></span>
		</div>
  </div>
   <div class="form-group row">
		<label>Date of addmission</label>
		<div class="col-sm-10">
			<input asp-for="DateOfAdmission"/>
			<span asp-validation-for="DateOfAdmission"></span>
		</div>
  </div>
  <div class="form-group row">
		<label>Date of issue</label>
		<div class="col-sm-10">
			<input asp-for="DateOfIssue"/>
			<span asp-validation-for="DateOfIssue"></span>
		</div>
  </div>
  <div class="col-sm-10 pt-2">
	  <button type="submit">Add order</button>
  </div>
</form>
